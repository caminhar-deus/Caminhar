{
  "name": "caminhar",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "check-env": "node lib/check-env.js",
    "dev": "npm run check-env && next dev --turbo",
    "build": "npm run check-env && next build --turbo",
    "postbuild": "next-sitemap",
    "start": "npm run check-env && next start",
    "dev:clean": "npx kill-port 3000 && rm -rf .next && rm -f babel.config.js babel.config.cjs && npm run dev",
    "analyze": "next build --analyze",
    "lint": "next lint",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "init-db": "node lib/init-server.js",
    "init-backup": "node lib/init-backup.js",
    "create-backup": "node -e \"import('./lib/backup.js').then(m => m.createBackup()).catch(console.error)\"",
    "restore-backup": "node lib/restore-backup.js",
    "init-posts": "node lib/init-posts.js",
    "clean-test-db": "node lib/clean-test-db.js",
    "test:load": "k6 run load-tests/health-check.js",
    "test:load:auth": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=password load-tests/authenticated-flow.js",
    "test:load:write": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=password load-tests/create-post-flow.js",
    "clean:load-posts": "node lib/clean-load-test-posts.js",
    "migrate-db": "node lib/migrate-sqlite-pg.js",
    "test:load:write-and-clean": "npm run test:load:write && npm run clean:load-posts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caminhar-deus/Caminhar.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caminhar-deus/Caminhar/issues"
  },
  "homepage": "https://github.com/caminhar-deus/Caminhar#readme",
  "dependencies": {
    "@upstash/redis": "^1.28.0",
    "axios": "^1.13.3",
    "bcryptjs": "^3.0.3",
    "cookie": "^1.1.1",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.5",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.3",
    "next": "^16.1.4",
    "pg": "^8.11.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "sqlite": "^5.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "node-mocks-http": "^1.14.1"
  },
  "overrides": {
    "tar": "^7.4.3"
  }
}
