{
  "name": "caminhar",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "check-env": "node lib/check-env.js",
    "dev": "npm run check-env && next dev --turbo",
    "build": "npm run check-env && next build --turbo",
    "postbuild": "next-sitemap",
    "start": "npm run check-env && next start",
    "dev:clean": "npx kill-port 3000 && rm -rf .next && rm -f babel.config.js babel.config.cjs && npm run dev",
    "analyze": "next build --analyze",
    "lint": "next lint",
    "find-unused": "node lib/find-unused.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=1",
    "init-db": "node lib/init-server.js",
    "init-backup": "node lib/init-backup.js",
    "create-backup": "node -e \"import('./lib/backup.js').then(m => m.createBackup()).catch(console.error)\"",
    "restore-backup": "node lib/restore-backup.js",
    "init-posts": "node lib/init-posts.js",
    "init-videos": "node lib/init-videos.js",
    "clean-test-db": "node lib/clean-test-db.js",
    "test:load": "k6 run load-tests/health-check.js",
    "test:load:auth": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/authenticated-flow.js",
    "test:load:write": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/create-post-flow.js",
    "clean:load-posts": "node lib/clean-load-test-posts.js",
    "test:load:write-and-clean": "npm run test:load:write && npm run clean:load-posts",
    "test:load:videos": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/videos-load-test.js",
    "test:load:videos-crud": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/videos-crud-test.js",
    "test:load:all": "npm run test:load:auth && npm run test:load:write-and-clean && npm run test:load:videos && npm run test:load:videos-crud",
    "test:load:upload": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/upload-flow.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caminhar-deus/Caminhar.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caminhar-deus/Caminhar/issues"
  },
  "homepage": "https://github.com/caminhar-deus/Caminhar#readme",
  "dependencies": {
    "@upstash/redis": "^1.36.2",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "client-only": "^0.0.1",
    "cookie": "^1.1.1",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.3.1",
    "form-data": "^4.0.5",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.3",
    "next": "^16.1.6",
    "pg": "^8.18.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-easy-crop": "^5.5.6",
    "react-hot-toast": "^2.6.0",
    "sqlite": "^5.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@faker-js/faker": "^10.3.0",
    "@jest/reporters": "^30.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "next-sitemap": "^4.2.3",
    "node-mocks-http": "^1.17.2",
    "package": "^1.0.1",
    "undici": "^7.22.0"
  },
  "overrides": {
    "tar": "^7.4.3",
    "glob": "^13.0.6",
    "minimatch": "^10.2.2"
  }
}
