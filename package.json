{
  "name": "caminhar",
  "version": "1.4.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "check-env": "node scripts/check-env.js",
    "dev": "npm run check-env && next dev --turbo",
    "build": "npm run check-env && next build --turbo",
    "postbuild": "next-sitemap",
    "start": "npm run check-env && next start",
    "dev:clean": "npx kill-port 3000 && rm -rf .next && rm -f babel.config.js babel.config.cjs && npm run dev",
    "analyze": "next build --analyze",
    "lint": "next lint",
    "find-unused": "node scripts/find-unused.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=1",
    "db:init": "node scripts/init-server.js",
    "db:status": "node scripts/check-db-status.js",
    "db:shell": "node scripts/db-shell.js",
    "db:reset": "node scripts/init-posts.js && node scripts/init-videos.js && node scripts/init-musicas.js && npm run db:init",
    "db:reset:safe": "npm run backup:create && npm run db:reset",
    "db:clean:test": "node scripts/clean-test-db.js",
    "backup:init": "node scripts/init-backup.js",
    "backup:create": "node scripts/create-backup.js",
    "backup:restore": "node scripts/restore-backup.js",
    "seed:all": "node scripts/seed-all.js",
    "migrate": "node scripts/migrations/001-add-views-to-posts.js",
    "validate-schema": "node scripts/validate-schema.js",
    "monitor:disk": "node scripts/monitor-disk-space.js",
    "clean:reports": "node scripts/clean-k6-reports.js",
    "clean:images": "node scripts/clean-orphaned-images.js",
    "test:load": "k6 run load-tests/health-check.js",
    "test:load:auth": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/authenticated-flow.js",
    "test:load:write": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/create-post-flow.js",
    "test:load:write-and-clean": "npm run test:load:write && node scripts/clean-load-test-posts.js",
    "test:load:videos": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/videos-load-test.js",
    "test:load:videos-crud": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/videos-crud-test.js",
    "test:load:musicas-crud": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/musicas-crud-test.js",
    "test:load:all": "npm run clean:reports && npm run test:load:auth && npm run test:load:write-and-clean && npm run test:load:videos && npm run test:load:videos-crud && npm run test:load:musicas-crud",
    "test:load:safe": "bash scripts/run-load-tests.sh",
    "report:load": "node scripts/generate-load-report.js",
    "test:load:upload": "k6 run -e ADMIN_USERNAME=admin -e ADMIN_PASSWORD=123456 load-tests/upload-flow.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caminhar-deus/Caminhar.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caminhar-deus/Caminhar/issues"
  },
  "homepage": "https://github.com/caminhar-deus/Caminhar#readme",
  "dependencies": {
    "@upstash/redis": "^1.36.2",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "client-only": "^0.0.1",
    "cookie": "^1.1.1",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.3.1",
    "form-data": "^4.0.5",
    "formidable": "^3.5.4",
    "jsonwebtoken": "^9.0.3",
    "next": "^16.1.6",
    "pg": "^8.18.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-easy-crop": "^5.5.6",
    "react-hot-toast": "^2.6.0",
    "sqlite": "^5.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@faker-js/faker": "^10.3.0",
    "@jest/reporters": "^30.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "next-sitemap": "^4.2.3",
    "node-mocks-http": "^1.17.2",
    "package": "^1.0.1",
    "undici": "^7.22.0"
  },
  "overrides": {
    "tar": "^7.4.3",
    "glob": "^13.0.6",
    "minimatch": "^10.2.2"
  }
}
